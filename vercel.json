{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/.*",
            "dest": "/api"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}